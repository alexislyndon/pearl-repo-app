<div class="success-msg">This is a success message.</div>
<table class="main-tbl" id="workgroups">
  <thead>
    <tr>
      <th>Workgroup Name</th>
      <th>WG Admins</th>
    </tr>
  </thead>
  <tbody>
    <% workgroups.forEach(element => { %>
    <tr data-id="<%= element.wg_id %>">
      <td><%= element.wgname %></td>

      <td>
        <% element.users.forEach(u => { %>
        <p data-user-uuid="<%= u.uuid %>"><%= u.user %></p>
        <% }) %>
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>